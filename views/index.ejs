<!DOCTYPE html>
<html>
  <head>
    <title>Image Upload</title>
  </head>
  <body>
    <% if (msg) { %>
    <p><%= msg %></p>
    <% } %>
    <form action="/upload" method="POST" enctype="multipart/form-data">
      <input type="file" name="myImage" />
      <input type="submit" value="Upload" />
    </form>
    <% if (file) { %>
    <img src="<%= file %>" />
    <% } %>
  </body>
</html>
